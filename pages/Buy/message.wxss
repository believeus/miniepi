.page {
  width: 100%;
}

.page .top {
  width: 90%;
}

.page .top .context {
  float: right;
}

.page .part1 .title {
  font-size: 20px;
}

.page .part1 .context {
  font-size: 14px;
}

.page .part1 .button {
  width: 30%;
  font-size: 12px;
  display: inline-block;
  transform: translateY(-15rpx);
  margin: 0rpx 10px;
}

.br {
  width: 100%;
  height: 15rpx;
  background-color: #f4f6f8;
}

.page .bottom {
  height: 50px;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.page .bottom .left {
  text-align: center;
  background-color: #37475d;
  width: 60%;
  color: #f4f6f8;
}

.page .bottom .right {
  width: 40%;
  background-color: #0071b1;
  text-align: center;
  color: #f4f6f8;
}
