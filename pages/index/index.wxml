<view class='page' style='width:{{width}}px;height:667px'>
  <view style='height:1.5%;width:100%;'></view>
  <view style='width:100%;height:74px;'>
    <view style='height:100%;display: flex;flex-direction: row;;'>
      <view style='height:100%;width:5%;;'></view>
      <view style='height:100%;width:70%;'>
        <view style='height:45%;width:100%;color:#4d4d4d;font-size:22px;font-weight:700;;transform: translateY(-3px)'>首页</view>
        <view style='height:4%;width:100%;;'></view>
        <view style='height:25%;width:100%;font-size:12px;color:#808080;;'>游客，您好！</view>
      </view>
      <view style='height:100%;width:20%;;'>
        <view style='height:25%;width:100%;;font-size:12px;color:#808080;display:flex;'>
          <view style='height:100%;width:36%;'></view>
          <view style='height:100%;width:56%;;'>
            <!-- <view class='select_box' style='border:1px solid #e2e2e2;'>
              <view class='select' catchtap='selectTap'>
                <text class='select_text'>{{selectData[index]}}</text>
                <image class='select_img {{selectShow&&"select_img_rotate"}}' src='http://47.75.111.0/data/image/index/allow.png' background-size="contain"></image>
              </view>
              <view class='option_box' style='height:{{selectShow?(selectData.length>5?325:selectData.length*50):0}}rpx;'>
                <text class='option' wx:for='{{selectData}}' wx:key='this' data-index='{{index}}' catchtap='optionTap'>{{item}}</text>
              </view>
            </view> -->
          </view>
        </view>
        <view style='height:10%;width:100%;;font-size:12px;color:#808080;;'>
        </view>
        <view style='height:48%;width:100%;;display:flex'>
          <view style='height:100%;width:49%'></view>
          <image src='http://47.75.111.0/data/image/index/user-logo.png' mode='widthFix' style='width:50%'></image>
        </view>
      </view>
      <view style='height:100%;width:4%;'></view>
    </view>
  </view>

  <view style='width:100%;height:147px;;display: flex;flex-direction: row;justify-content: center;'>
    <view style='width:90%;height:100%;;'>
      <swiper style="height:100%;width:100%; " autoplay='true' interval='2000' duration='500' circular='true'>
        <block wx:for="{{Urls}}">
          <swiper-item>
            <image src="{{item.img}}" mode='widthFix' style="width:100%;" bindtap='ToSwiper' data-urls='{{item.url}}' />
          </swiper-item>
        </block>
      </swiper>
    </view>
  </view>

  <view style='width:100%;height:10px;background-color:#eaeaea'></view>

  <view style='width: 100%;height: 313.5px;background-color: #f9f9f9;'>
    <view style='width:100%;height:5%;'></view>
    <view style='width:100%;height:95%;;display:flex;'>
      <view style='width:5%;height:100%;;'></view>
      <view style='width:90%;height:100%;;'>
        <view style='width:100%;height:50%;;display:flex;'>
          <view style='width: 47.7%;height: 100%;'>
            <image src='http://47.75.111.0/data/image/index/IntroductionofProducts.png' style='width:100%;height:93%' bindtap='ToIntroduction'></image>
          </view>
          <view style='width:4%;height:100%;;'></view>
          <view style='width: 47.7%;height: 100%;'>
            <image src='http://47.75.111.0/data/image/index/TestProcess.png' style='width:100%;height:93%' bindtap='ToTestProcess'></image>
          </view>
        </view>
        <view style='width: 42%;height: 0;padding-top: 42%;top : 318px;left : 29%;border-radius: 100%;box-sizing: border-box;position: absolute;background-color:#ffffff;'>


          <view id='cicle' style='width:{{L}}px;height:{{L}}px'>
            <view style="width:{{L}}px;height:{{L}}px;border-radius:{{L/2}}px;border:1px solid #000;position:absolute;left:0; right:0; top:0; bottom:0;margin:auto;">
              <view style="width:{{L/2}}px;height:{{L}}px;border-radius:{{L/2}}px 0 0 {{L/2}}px;position:absolute;left:0px;top:0px;z-index:10;background-image: url('{{TaiChi_1}}');background-size:100% 100%; background-repeat:no-repeat" bindtap='ToReport' >
              </view>

              <view style=" width:{{L/2}}px;height:{{L}}px;border-radius:0 {{L/2}}px {{L/2}}px 0;position:absolute;left:{{L/2}}px;top:0px;background-color:#000;z-index:10; background-image: url('{{TaiChi_2}}');background-size:100% 100%; background-repeat:no-repeat" bindtap='ToLife'>
              </view>

              <view style="width:{{L/2}}px;height:{{L/2}}px;border-radius:{{L/2}}px;position:absolute;left:{{L/2-L/4}}px;top:0px;z-index:25;background-color:#000;color:#ffffff" bindtap='ToLife'>
              </view>

              <view style="width:{{L/2}}px;height:{{L/2}}px;border-radius:{{L/2}}px {{L/2}}px {{L/2}}px {{L/2}}px;position:absolute;left:{{L/2-L/4}}px;top:{{L/2}}px;z-index:25;background-color:#ffffff" bindtap='ToReport'>
              </view>

              <view style="width:{{L/4}}px;height:{{L/4}}px;border-radius:{{L/4}}px;position:absolute;left:{{L/2-L/8}}px;top:{{L/2+L/8}}px;z-index:25;background-color:#000000" bindtap='ToReport'></view>

              <view style="width:{{L/4}}px;height:{{L/4}}px;border-radius:{{L/4}}px;position:absolute;left:{{L/2-L/8}}px;top:{{L/8}}px;z-index:25;background-color:#ffffff" bindtap='ToLife'></view>
            </view>
          </view>

        </view>
        <view style='width:100%;height:50%;;display:flex'>
          <view style='width: 47.7%;height: 100%;'>
            <image src='http://47.75.111.0/data/image/index/HealthMarketPlace.png' style='width:100%;height:93%' bindtap='ToMarket'></image>
          </view>
          <view style='width:4%;height:100%;'></view>
          <view style='width: 47.7%;height: 100%;'>
            <image src='http://47.75.111.0/data/image/index/YourLifeStyle.png' style='width:100%;height:93%' bindtap='ToQuestionnaire'></image>
          </view>
        </view>
      </view>
      <view style='width:5%;height:100%;'></view>
    </view>
  </view>

  <view style='width: 100%;height: 38%;background-color: #f9f9f9;;display:flex'>
    <view style='width:5%;height:100%'></view>
    <view style='width:90%;height:100%'>
      <view style='width:100%;height:72.5px;;margin-top:10px;display:flex'>
        <view style='width:47.7%;height:100%;'>
          <image src='http://47.75.111.0/data/image/index/1.png' mode='widthFix' style='width:100%' bindtap='ToBio'></image>
        </view>
        <view style='width:4%;height:100%;'></view>
        <view style='width:47.7%;height:100%;'>
          <image src='http://47.75.111.0/data/image/index/2.png' mode='widthFix' style='width:100%' bindtap='ToSame'></image>
        </view>
      </view>

      <view style='width:100%;height:72.5px;;margin-top:10px'>
        <image src='http://47.75.111.0/data/image/index/OrderRecord.png' mode="widthFix" style='width:100%' bindtap='ToOrderList'></image>
      </view>

      <view style='width:100%;height:72.5px;;margin-top:10px'>
        <image src='http://47.75.111.0/data/image/index/CommonProblems.png' mode="widthFix" style='width:100%' bindtap='ToFAQ'></image>
      </view>
    </view>
    <view style='width:5%;height:100%;;'></view>
  </view>

  <view style='width:100%;height:5%;display: flex;flex-direction: row;justify-content: center;  background-color: #f9f9f9;'>
    <view style='background-color: #f9f9f9;color:#959595;font-size:12px;margin-top:2%'>没有更多了</view>
  </view>
</view>