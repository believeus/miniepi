<!--pages/questionnaire/index.wxml-->
<!--url="../questionnaire/mess?type_code={{item.type_code}}&type_id={{item.type_id}}"-->
<view wx:for="{{data}}" wx:for-index="index" wx:for-item="item">
  <navigator class="order-detail" url="../questionnaire/mess?type_code={{item.type_code}}&type_id={{item.type_id}}">
    <view class='page' style='margin-bottom:10px'>
      <view style='width:100%;'>
        <scroll-view scroll-y='true' class='scroll'>
          <view style='width: 99%;height:70px;border:2px solid #000'>
            <view style='width:100%;height:70px;'>
              <swiper style='height:70px;width:100%'>
                <block>
                  <swiper-item>
                    <view class='checkbox'>
                      <view class='title'>
                        {{item.type_name}}-{{item.type_descs}}
                      </view>
                    </view>
                  </swiper-item>
                </block>
              </swiper>
            </view>
          </view>
        </scroll-view>
      </view>
    </view>
  </navigator>
</view>
